
<%- include ('../partials/head'); %> <!-- Importando com o ejs uma pagina -->

<body class="container">
    <%- include ('../partials/header'); %>
    <main>
        <div class="container-fluid bg-light">
            <h1>Treino com EJS</h1>
            <p>Treinando com node + EJS</p>
            <p><%= subtitle %></p> <!-- Olhar no arquivo server.js -->
        </div>
<!-- Treinando com forEach pegando cada elemento de um array no express (olhar o server.js) -->
        <ul>
            <% quality.forEach(function(quality) { %>
                <li><strong><%= quality.title %></strong><%= quality.message %></li> 

           <% }) %>
        </ul>
    </main>
    <%- include ('../partials/footer'); %>

</body>
</html>
